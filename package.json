          "*bp/texts/languages.{json,jsonc,json5}",
          "bp_*/texts/languages.{json,jsonc,json5}",
          "resource_pack/texts/languages.{json,jsonc,json5}",
          "*RP/texts/languages.{json,jsonc,json5}",
          "RP_*/texts/languages.{json,jsonc,json5}",
          "*rp/texts/languages.{json,jsonc,json5}",
